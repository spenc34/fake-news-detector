{
    "manifest_version": 2,
  
    "name": "Fake News Detector",
    "description": "Uses machine learning to detect whether a news site is unreliable",
    "version": "0.0.1",
    "browser_action": {
      "default_popup": "index.html",
      "default_title": "Fake News Detector",
      "default_icon": {
        "16": "icons8-news-16.png",
        "32": "icons8-news-32.png",
        "48": "icons8-news-48.png",
        "128": "icons8-news-128.png"
      }
    },
    "icons": {
      "16": "icons8-news-16.png",
      "32": "icons8-news-32.png",
      "48": "icons8-news-48.png",
      "128": "icons8-news-128.png"
    },
    "permissions": [
      "tabs", "activeTab", "storage"
    ]
  }